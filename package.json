{
  "name": "cory",
  "version": "1.2.0",
  "description": "Tiny generator for static sites",
  "files": [
    "template",
    "dist"
  ],
  "scripts": {
    "prepublish": "gulp compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leo/cory.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-runtime",
      "syntax-async-functions"
    ]
  },
  "keywords": [
    "static",
    "site",
    "generator"
  ],
  "author": "Leonard Lamprecht <mindrun@icloud.com> (http://leo.im)",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "cory": "dist/bin/index",
    "cory-serve": "dist/bin/serve",
    "cory-build": "dist/bin/build",
    "cory-init": "dist/bin/init",
    "cory-clean": "dist/bin/clean",
    "cory-deploy": "dist/bin/deploy"
  },
  "greenkeeper": {
    "emails": false
  },
  "bugs": {
    "url": "https://github.com/leo/cory/issues"
  },
  "homepage": "https://github.com/leo/cory#readme",
  "dependencies": {
    "args": "^0.1.1",
    "babel-runtime": "^6.6.1",
    "broccoli": "^0.16.9",
    "broccoli-sane-watcher": "^1.1.4",
    "browser-sync": "^2.12.5",
    "chokidar": "^1.4.3",
    "colors": "^1.1.2",
    "front-matter": "^2.0.7",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "markdown": "^0.5.0",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "nodegit": "^0.12.2",
    "open": "0.0.5",
    "ora": "^0.2.1",
    "update-notifier": "^0.7.0",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-ext": "^1.0.0"
  }
}
